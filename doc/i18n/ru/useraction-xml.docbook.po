#. Tag: para
#: useraction-xml.docbook:94
#, no-c-format
msgid ""
"Search and replace a strings and regular expressions in textfiles using "
"<command>sed</command>:"
msgstr ""

#. Tag: programlisting
#: useraction-xml.docbook:95
#, no-c-format
msgid ""
"&lt;?xml version=\"1.0\" encoding=\"UTF-8\" ?>      \n"
"&lt;!DOCTYPE KrusaderUserActions>\n"
"&lt;KrusaderUserActions>\n"
" &lt;action name=\"search_replace\" >\n"
"  &lt;title>Simple Search and Replace&lt;/title>\n"
"  &lt;tooltip>Search and replace a string/regex in textfiles&lt;/tooltip>\n"
"  &lt;icon>kcachegrind&lt;/icon>\n"
"  &lt;category>Samples&lt;/category>\n"
"  &lt;description>This UA uses the UNIX standard-tool sed (the stream-"
"editor) so perform search and replace operations in all selected files It is "
"possible to give a regular expression for both strings. Please note: The sed-"
"command is \"s/search/repleace/\", so you have to escape the slash when you "
"need it!&lt;/description>\n"
"  &lt;command>sed -i -e 's/%_Ask(\"Search for:\")%/%_Ask(\"Replace with:"
"\")%/' %aList(\"Selected\")%&lt;/command>\n"
" &lt;/action>\n"
"&lt;/KrusaderUserActions>"
msgstr ""

#. Tag: para
#: useraction-xml.docbook:99
#, no-c-format
msgid ""
"Converts content of selected text-files from ISO-8859-15 to UTF8 and vice "
"versa using <command>iconv</command> (also inside archives):"
msgstr ""

#. Tag: programlisting
#: useraction-xml.docbook:101
#, no-c-format
msgid ""
"&lt;?xml version=\"1.0\" encoding=\"UTF-8\" ?>      \n"
"&lt;!DOCTYPE KrusaderUserActions>\n"
"&lt;KrusaderUserActions> \n"
" &lt;action name=\"iso8859-15_to_utf8\" > \n"
"  &lt;title>ISO 8859-15 => UTF8&lt;/title> \n"
"  &lt;tooltip>convert characterset&lt;/tooltip> \n"
"  &lt;icon>editcopy&lt;/icon> \n"
"  &lt;category>Selection&lt;/category> \n"
"  &lt;description>Converts content of selected text-files from ISO-8859-15 "
"to UTF8&lt;/description> \n"
"  &lt;command>sh -c 'cd %aPath%; for i in %aList(&quot;Selected&quot;, "
"&quot; &quot;, &quot;Yes&quot;)% ; do iconv -f iso8859-15 -t utf-8 &quot;"
"$i&quot; -o %oPath%&quot;$i&quot;; done '&lt;/command> \n"
" &lt;/action> \n"
" &lt;action name=\"utf8_to_iso8859-15\" > \n"
"  &lt;title>UTF8 => ISO 8859-15&lt;/title> \n"
"  &lt;tooltip>convert characterset&lt;/tooltip> \n"
"  &lt;icon>editcopy&lt;/icon> \n"
"  &lt;category>Selection&lt;/category> \n"
"  &lt;description>Converts content of selected text-files from UTF8 to ISO-"
"8859-15&lt;/description> \n"
"  &lt;command>sh -c 'cd %aPath%; for i in %aList(&quot;Selected&quot;, "
"&quot; &quot;, &quot;Yes&quot;)% ; do iconv -f utf-8 -t iso8859-15 &quot;"
"$i&quot; -o %oPath%&quot;$i&quot;; done '&lt;/command> \n"
" &lt;/action> \n"
"&lt;/KrusaderUserActions>"
msgstr ""
