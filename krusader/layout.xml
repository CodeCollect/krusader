<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE KrusaderLayout>
<KrusaderLayout>
    <layout name="default" type="vertical">
        <layout type="horizontal">
            <widget>origin</widget>
            <widget>toolbar</widget>
        </layout>
        <layout type="horizontal">
            <widget>mediaButton</widget>
            <frame type="horizontal" color="Statusbar" shape="Box" shadow="Raised">
                <widget>status</widget>
            </frame>
            <widget>historyButton</widget>
            <widget>bookmarksButton</widget>
        </layout>
        <widget>vfsError</widget>
        <widget>clientArea</widget>
        <layout type="horizontal">
            <frame type="horizontal" color="Statusbar" shape="Box" shadow="Raised">
                <widget>totals</widget>
                <widget>freeSpace</widget>
            </frame>
            <widget>previewProgress</widget>
            <widget>inlineRefreshCancelButton</widget>
            <widget>popupBtn</widget>
        </layout>
    </layout>
</KrusaderLayout>
