<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE KrusaderLayout>
<KrusaderLayout>
    <layout name="default" type="vertical">
        <layout type="horizontal">
            <widget name="origin"/>
            <widget name="toolbar"/>
        </layout>
        <layout type="horizontal">
            <widget name="mediaButton"/>
            <frame type="horizontal" color="Statusbar" shape="Box" shadow="Raised">
                <widget name="status"/>
            </frame>
            <widget name="historyButton"/>
            <widget name="bookmarksButton"/>
        </layout>
        <widget name="vfsError"/>
        <widget name="clientArea"/>
        <layout type="horizontal">
            <frame type="horizontal" color="Statusbar" shape="Box" shadow="Raised">
                <widget name="totals"/>
                <widget name="freeSpace"/>
            </frame>
            <widget name="previewProgress"/>
            <widget name="inlineRefreshCancelButton"/>
            <widget name="popupBtn"/>
        </layout>
    </layout>
</KrusaderLayout>
